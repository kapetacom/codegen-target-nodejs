//#FILENAME:db/{{data.metadata.name}}/schema.prisma:create-only
// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
    output   = "../../src/data/clients/{{data.metadata.name}}"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Example {
    id    Int     @id @default(autoincrement())
    title String?
}