//#FILENAME:package.json:merge
{
  "name": "block-service-{{lowercase data.metadata.name}}",
  "version": "{{data.metadata.version}}",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "@blockware/sdk-config": "<2",
    "@blockware/sdk-server": "<2"
    {{#consumes 'blockware/resource-type-rest-client'}},
        "@blockware/sdk-rest-client": "<2"
    {{/consumes}}
    {{#provides 'blockware/resource-type-rest-api'}},
        "@blockware/sdk-rest-route": "<2"
    {{/provides}}
    {{#consumes 'blockware/resource-type-grpc-client'}},
        "@blockware/sdk-grpc-client": "<2"
    {{/consumes}}
    {{#provides 'blockware/resource-type-grpc-api'}},
        "@blockware/sdk-grpc-route": "<2"
    {{/provides}}
    {{#consumes 'blockware/resource-type-mongodb'}},
        "@blockware/sdk-nosql-mongodb": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-couchdb'}},
        "@blockware/sdk-nosql-couchdb": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-cassandra'}},
        "@blockware/sdk-nosql-cassandra": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-redis'}},
        "@blockware/sdk-nosql-redis": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-postgresql'}},
        "@blockware/sdk-sqldb-postgresql": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-mysql'}},
        "@blockware/sdk-sqldb-mysql": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-mssql'}},
        "@blockware/sdk-sqldb-mssql": "<2"
    {{/consumes}}
    {{#consumes 'blockware/resource-type-oracle'}},
        "@blockware/sdk-sqldb-oracle": "<2"
    {{/consumes}}

  }
}